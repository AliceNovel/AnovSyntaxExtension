{
  "name": "anov-syntax",
  "displayName": "Anov Syntax",
  "description": "Provides Anov Syntax to create novel games for Visual Studio Code",
  "publisher": "Alice Project",
  "keywords": [
    "Alice Project",
    "Alice Novel",
    "Alice Console",
    "Alice CLI",
    "Anov Syntax",
    "Novel Games",
    "Games"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AliceNovel/AnovSyntaxExtension"
  },
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "anov",
      "aliases": ["anov-syntax", "anov"],
      "extensions": [".anov"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "anov",
      "scopeName": "source.anov",
      "path": "./syntaxes/anov.tmLanguage.json"
    }],
    "snippets": [{
        "language": "anov",
        "scopeName":"source.anov",
        "path": "./snippet/anov.snippet.json"
    }]
  }
}
