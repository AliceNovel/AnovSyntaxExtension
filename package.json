{
  "name": "anov-syntax",
  "displayName": "Anov Syntax",
  "description": "Anov Syntax is the easy language for creating novel games.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "anov",
      "aliases": ["anov-syntax", "anov"],
      "extensions": [".anov"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "anov",
      "scopeName": "source.anov",
      "path": "./syntaxes/anov.tmLanguage.json"
    }]
  }
}
